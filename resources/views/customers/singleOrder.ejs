<section class="status">
    <div class="container mx-auto">
        <div class="w-2/3 mx-auto status-box">
            <div class="flex items-center justify-between mb-12 ml-16 pt-5">
                <h1 class="text-xl font-bold">Track your Order</h1>
                <h6 class="py-1 orderid_color px-4 text-xs mr-16"><%= order._id %></h6>

            </div>
            <ul class="pb-8">
                <li class="status_line text-xl pb-16"><span>Order placed</span></li>
                <li class="status_line text-xl pb-16"><span>Order confirmed</span></li>
                <li class="status_line text-xl pb-16"><span>Order prepared</span></li>
                <li class="status_line text-xl pb-16"><span>Out for delivery</span></li>
                <li class="status_line text-xl"><span>Order completed</span></li>
            </ul>
        </div>
    </div>
</section>